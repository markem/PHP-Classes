Imperial_Uncut_Printing_Paper_Double_Foolscap	17.0 x 27.0 in	431.8 x 685.8 mm	1:1.5882
EOT;

	$ary = [];
	$ary[] = array( "Name", "Width-mm", "Height-mm", "Width-in", "Height-in",
		"Width-MM2IN", "Height-MM2IN", "Ratio #1", "Ratio #2", "Info" );

	$a = exptrim( "\n", $a );
	foreach( $a as $k=>$v ){
#
#	Get a line
#
		$v = str_replace( "", "", $v );
#
#	If this is the title line - skip it
#
		if( preg_match("/size/i", $v) ){ continue; }
#
#	Split up the incoming line and get the count of it
#
		$b = exptrim( "	", $v );
$pr->pr( $b, "B = " );
		$bc = count( $b );
#
#	Get the name of the paper
#
		$name = $b[0];
#
#	Split up the first group of information and get the count of it
#
		$c = exptrim( ' ', $b[1] );
		$cc = count( $c );
		$ca = [];
#
#	Split up the second group of information and get the count of it
#
		$d = exptrim( ' ', $b[2] );
		$dc = count( $d );
#
#	Adjust the arrays so they are even
#
		while( $cc < $dc ){ $c[] = ""; $cc = count($c); }
		while( $dc < $cc ){ $d[] = ""; $dc = count($d); }
#
#	If the first line is for inches - exchange it with the millimeter one
#	Remember: Millimeters, Inches, MM2IN
#
		if( preg_match("/ in/i", $b[1]) ){
			for( $i=0; $i<$cc; $i++ ){
				$j = $c[$i]; $c[$i] = $d[$i]; $d[$i] = $j;
				}
			}
#
#	Do the Millimeter to Inches conversion.
#
		for( $i=0; $i<$cc; $i++ ){ $ca[$i] = $c[$i]; }
		if( is_numeric($ca[0]) ){ $ca[0] = $ca[0] / 25.4; }
		if( is_numeric($ca[2]) ){ $ca[2] = $ca[2] / 25.4; }

		if( $bc > 3 ){ $r1 = exptrim( ' ', $b[3] ); }
			else { $r1 = []; }

		if( $bc > 4 ){ $r2 = exptrim( ' ', $b[4] ); }
			else { $r2 = []; }

		if( $bc > 5 ){ $info = exptrim( ' ', $b[5] ); }
			else { $info = []; }
#
#	Now remove the 'x's
#
		unset( $c[1] );
		unset( $d[1] );
		unset( $ca[1] );
#
#	Now remove the mm/in
#
		unset( $c[3] );
		unset( $d[3] );
		unset( $ca[3] );

		echo "Name:$name\n";
		echo "Metric Measurements: ";
		foreach( $c as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo "Metric Converted to Inches Measurements: ";
		foreach( $ca as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo "Inch Measurements: ";
		foreach( $d as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo "Ratios #1: ";
		foreach( $r1 as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo "Ratios #2: ";
		foreach( $r2 as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo "Information: ";
		foreach( $info as $k1=>$v1 ){ echo "$v1, "; }
		echo "\n";
		echo str_repeat( "-", 80 ) . "\n";

		$ary[] = array( $name, $c[0], $c[2], $d[0], $d[2], $ca[0], $ca[2],
			$r1[0], $r2[0], $info );
		}

	$cf->fput_csv( "./paper.ini", $ary );
	echo "Finished\n";
	exit();

function exptrim( $chr, $a )
{
	$b = explode( $chr, $a );
	foreach( $b as $k=>$v ){
		$b[$k] = trim( $v );
		}

	if( $b[count($b)-1] == "m" ){
		$b[0] = $b[0] * 1000;
		$b[2] = $b[2] * 1000;
		$b[count($b)-1] = "mm";
		}

	return $b;
}
